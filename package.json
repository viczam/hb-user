{
  "name": "hb-user",
  "version": "0.5.0",
  "description": "Hapi.js user plugin that integrates octobus.js and octobus-rethinkdb",
  "main": "dist/index.js",
  "scripts": {
    "test:watch": "npm t -- -w",
    "test": "mocha --compilers js:babel-register --require ./test/helper.js",
    "prebuild": "rimraf dist",
    "build": "babel src --copy-files --out-dir dist",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viczam/hb-user.git"
  },
  "keywords": [
    "hapi.js",
    "Hapi",
    "rethinkdb",
    "octobus"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Victor Zamfir <zamfir.victor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/viczam/hb-user/issues"
  },
  "homepage": "https://github.com/viczam/hb-user#readme",
  "dependencies": {
    "babel-runtime": "6.6.1",
    "bcryptjs": "^2.3.0",
    "boom": "^3.1.2",
    "hapi-auth-jwt2": "^7.0.0",
    "joi": "8.0.5",
    "jsonwebtoken": "^5.7.0",
    "lodash": "4.11.2",
    "octobus.js": "0.11.1",
    "octobus-mongodb": "0.6.3"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "5.0.0",
    "babel-plugin-transform-async-to-generator": "6.7.4",
    "babel-plugin-transform-object-rest-spread": "6.6.5",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "dirty-chai": "^1.2.2",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.0",
    "hapi": "13.x.x",
    "hapi-octobus": "0.3.3",
    "mocha": "^2.4.5",
    "rimraf": "2.5.2",
    "semantic-release": "^4.3.5",
    "sinon": "^1.17.3",
    "sinon-chai": "2.8.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-async-to-generator",
      "transform-runtime"
    ]
  }
}
