{
  "name": "hb-user",
  "version": "0.8.0",
  "description": "Hapi.js user plugin that integrates octobus.js and octobus-rethinkdb",
  "main": "dist/index.js",
  "scripts": {
    "test:watch": "npm t -- -w",
    "test": "mocha --compilers js:babel-register --require ./test/helper.js",
    "prebuild": "rimraf dist",
    "build": "babel src --copy-files --out-dir dist",
    "prepublish": "npm run build",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viczam/hb-user.git"
  },
  "keywords": [
    "hapi.js",
    "Hapi",
    "rethinkdb",
    "octobus"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Victor Zamfir <zamfir.victor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/viczam/hb-user/issues"
  },
  "homepage": "https://github.com/viczam/hb-user#readme",
  "dependencies": {
    "babel-runtime": "6.6.1",
    "bcryptjs": "^2.3.0",
    "boom": "~4.0.0",
    "hapi-auth-jwt2": "~7.1.3",
    "joi": "~9.0.4",
    "jsonwebtoken": "~7.1.9",
    "lodash": "~4.15.0",
    "octobus.js": "0.16.1",
    "octobus-mongodb": "0.7.1"
  },
  "devDependencies": {
    "babel-cli": "~6.14.0",
    "babel-core": "~6.14.0",
    "babel-eslint": "~6.1.2",
    "babel-plugin-transform-async-to-generator": "~6.8.0",
    "babel-plugin-transform-object-rest-spread": "~6.8.0",
    "babel-plugin-transform-runtime": "~6.15.0",
    "babel-preset-es2015": "~6.14.0",
    "babel-preset-stage-0": "~6.5.0",
    "babel-register": "~6.14.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "dirty-chai": "^1.2.2",
    "eslint": "~3.5.0",
    "eslint-config-airbnb": "~11.1.0",
    "eslint-plugin-import": "1.15.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "~6.2.2",
    "estraverse-fb": "1.3.1",
    "hapi": "15.x.x",
    "hapi-octobus": "0.4.1",
    "mocha": "~3.0.2",
    "rimraf": "~2.5.4",
    "semantic-release": "^4.3.5",
    "sinon": "~1.17.5",
    "sinon-chai": "2.8.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-async-to-generator",
      "transform-runtime"
    ]
  }
}
