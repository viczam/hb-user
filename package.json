{
  "name": "hb-user",
  "version": "0.8.2",
  "description": "Hapi.js user plugin that integrates octobus.js and octobus-rethinkdb",
  "main": "dist/index.js",
  "scripts": {
    "test:watch": "npm t -- -w",
    "test": "mocha --compilers js:babel-register --require ./test/helper.js",
    "prebuild": "rimraf dist",
    "build": "babel src --copy-files --out-dir dist",
    "prepublish": "npm run build",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viczam/hb-user.git"
  },
  "keywords": [
    "hapi.js",
    "Hapi",
    "rethinkdb",
    "octobus"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Victor Zamfir <zamfir.victor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/viczam/hb-user/issues"
  },
  "homepage": "https://github.com/viczam/hb-user#readme",
  "dependencies": {
    "bcryptjs": "~2.3.0",
    "boom": "~4.2.0",
    "hapi-auth-jwt2": "~7.2.0",
    "joi": "~10.0.1",
    "jsonwebtoken": "~7.1.9",
    "lodash": "~4.17.2",
    "octobus-mongodb": "~0.8.1",
    "octobus.js": "~0.18.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "~6.18.2",
    "babel-eslint": "~7.1.1",
    "babel-plugin-transform-async-to-generator": "~6.16.0",
    "babel-plugin-transform-object-rest-spread": "~6.19.0",
    "babel-plugin-transform-runtime": "~6.15.0",
    "babel-preset-es2015": "~6.18.0",
    "babel-preset-stage-0": "~6.16.0",
    "babel-register": "~6.18.0",
    "babel-runtime": "~6.18.0",
    "chai": "~3.5.0",
    "chai-as-promised": "~6.0.0",
    "dirty-chai": "~1.2.2",
    "eslint": "~3.10.2",
    "eslint-config-airbnb": "~13.0.0",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-jsx-a11y": "~2.2.3",
    "eslint-plugin-react": "~6.7.1",
    "estraverse-fb": "~1.3.1",
    "hapi": "~15.2.0",
    "hapi-octobus": "~0.4.2",
    "mocha": "~3.1.2",
    "rimraf": "~2.5.4",
    "semantic-release": "~6.3.2",
    "sinon": "~1.17.6",
    "sinon-chai": "~2.8.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-async-to-generator",
      "transform-runtime"
    ]
  }
}
